{
  "rewrites": [
    {
      "source": "/gitbridge",
      "destination": "https://git-bridge.vercel.app/"
    },
    {
      "source": "/gitbridge/:path*",
      "destination": "https://git-bridge.vercel.app/:path*"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  }
}

